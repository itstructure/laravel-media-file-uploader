{
    "name": "itstructure/laravel-media-file-uploader",
    "description": "Media file uploader for laravel framework",
    "type": "library",
    "keywords": ["laravel", "media", "file", "upload", "image", "picture", "audio", "video"],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Andrey Girnik",
            "email": "girnikandrey@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel/framework": "^10.0",
        "itstructure/laravel-grid-view": "~1.1.0"
    },
    "autoload": {
        "psr-4": {
            "Itstructure\\MFU\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Itstructure\\MFU\\UploadServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
